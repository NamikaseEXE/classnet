{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run Next.js Dev Server",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/next",
            "args": [
                "dev"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "env": {
                "NODE_ENV": "development"
            }
        }
    ]
}